int increment(int i, int inc)
{
int k = i + inc;
return k;
}

int main()
{
int number = 4;
int plusValue = 2;
int returnValue = increment(number,plusValue);
char buffer[50];
char output[] = "%d";
char* outputPointer = @output[0];
sprintf(buffer[],outputPointer,returnValue);
printf(buffer[]);
return 0;
}
